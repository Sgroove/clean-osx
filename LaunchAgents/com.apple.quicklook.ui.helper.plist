<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.apple.quicklook.ui.helper</string>
	<key>ProgramArguments</key>
	<array>
		<string>/System/Library/Frameworks/Quartz.framework/Frameworks/QuickLookUI.framework/Resources/QuickLookUIHelper.app/Contents/MacOS/QuickLookUIHelper</string>
	</array>
	<key>MachServices</key>
	<dict>
		<key>com.apple.quicklook.ui.helper</key>
		<true/>
		<key>com.apple.quicklook.ui.helper.active</key>
		<dict>
			<key>HideUntilCheckIn</key>
			<true/>
			<key>ResetAtClose</key>
			<true/>
		</dict>
	</dict>
	<key>WorkingDirectory</key>
	<string>/tmp</string>
	<key>ThrottleInterval</key>
	<integer>1</integer>
	<key>EnableTransactions</key>
	<true/>
    <key>EnablePressuredExit</key>
    <true/>
	<key>POSIXSpawnType</key>
	<string>Interactive</string>
</dict>
</plist>
